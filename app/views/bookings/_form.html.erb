<%= form_with(model: [ @service, @service.build_booking]) do |form| %>
  <div class = "form-group">
    <%= form.label :address %><br>
    <%= form.text_field :address , class: "form-control" %>
  </div>
  <div class = "form-group">
    <%= form.label :contact %><br>
    <%= form.text_field :contact , class: "form-control" %>
  </div>
   <div class = "form-group">
    <%= form.label :time %><br>
    <%= form.datetime_local_field :time , class: "form-control" %>
  </div>
   <%= form.hidden_field :admin_user_id, value: current_admin_user.id%>
  <p>
    <%= form.submit class: "btn btn-success" %>
  </p>
<% end %>